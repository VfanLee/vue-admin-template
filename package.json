{
  "private": true,
  "name": "vue-admin-template",
  "description": "Vue Admin Template",
  "version": "1.0.0",
  "author": "Vfan Lee",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vite build --config ./config/vite.config.prod.ts",
    "report": "vite build --mode report --config ./config/vite.config.prod.ts",
    "preview": "npm run build && vite preview --host",
    "prepare": "husky install",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.44.7",
    "@tiptap/extension-bubble-menu": "^3.11.0",
    "@tiptap/pm": "^3.11.0",
    "@tiptap/starter-kit": "^3.11.0",
    "@tiptap/vue-3": "^3.11.0",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.5",
    "echarts": "^6.0.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.40",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash": "^4.14.186",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^22.18.11",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.14.0",
    "@types/sortablejs": "^1.15.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "less": "^4.1.3",
    "lint-staged": "^16.2.7",
    "mockjs": "^1.1.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.1.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
