{
  "private": true,
  "name": "vue-admin-template",
  "description": "Vue Admin Template",
  "version": "1.0.0",
  "author": "Vfan Lee",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vite build --config ./config/vite.config.prod.ts",
    "report": "vite build --mode report --config ./config/vite.config.prod.ts",
    "preview": "npm run build && vite preview --host",
    "prepare": "husky install",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@tinymce/tinymce-vue": "^6.3.0",
    "@tiptap/extension-bubble-menu": "^3.11.1",
    "@tiptap/pm": "^3.11.1",
    "@tiptap/starter-kit": "^3.11.1",
    "@tiptap/vue-3": "^3.11.1",
    "@typescript-eslint/parser": "^8.48.0",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.25",
    "vue-echarts": "^8.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.6",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/lodash": "^4.17.21",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.19.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@types/sortablejs": "^1.15.9",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "less": "^4.4.2",
    "lint-staged": "^16.2.7",
    "mockjs": "^1.1.0",
    "prettier": "^3.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.1.5"
  }
}
